                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </term>
        </config>
        <pd:inputBindings>
            <Data>
                <xsl:for-each select="$CommentData/AccumulatedOutput/pfx5:CommentDataCaseElement">
                    <pfx5:CommentDataCaseElement>
                        <xsl:if test="comment_Id">
                            <comment_Id>
                                <xsl:value-of select="comment_Id"/>
                            </comment_Id>
                        </xsl:if>
                        <siebelCircuitID>
                            <xsl:value-of select="siebelCircuitID"/>
                        </siebelCircuitID>
                        <xsl:if test="taskName">
                            <taskName>
                                <xsl:value-of select="taskName"/>
                            </taskName>
                        </xsl:if>
                        <xsl:if test="addedby">
                            <addedby>
                                <xsl:value-of select="addedby"/>
                            </addedby>
                        </xsl:if>
                        <xsl:if test="comments">
                            <comments>
                                <xsl:value-of select="comments"/>
                            </comments>
                        </xsl:if>
                        <xsl:if test="time">
                            <time>
                                <xsl:value-of select="time"/>
                            </time>
                        </xsl:if>
                    </pfx5:CommentDataCaseElement>
                </xsl:for-each>
            </Data>
        </pd:inputBindings>
    </pd:activity>
