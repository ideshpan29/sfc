            <xs:element name="InvalidEntityTypeFault" type="comex:DetailedException"/>
            <xs:element name="InvalidLDAPContainerFault" type="comex:DetailedException"/>
            <xs:element name="InvalidLDAPFault" type="comex:DetailedException"/>
            <xs:element name="InvalidLDAPResourceFault" type="comex:DetailedException"/>
            <xs:element name="InvalidLDAPSearchFault" type="comex:DetailedException"/>
