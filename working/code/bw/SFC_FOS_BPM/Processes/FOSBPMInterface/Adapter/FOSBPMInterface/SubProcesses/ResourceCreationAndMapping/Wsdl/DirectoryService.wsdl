        <xs:schema xmlns="http://exception.api.de.n2.tibco.com" xmlns:comex="http://exception.api.common.n2.tibco.com" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://exception.api.de.n2.tibco.com" version="1.0" elementFormDefault="unqualified" attributeFormDefault="unqualified">
