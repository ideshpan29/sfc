<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>Building Mapping DB Connection</name>
    <resourceType>ae.shared.JDBCSharedResource</resourceType>
    <config>
        <driver>oracle.jdbc.driver.OracleDriver (thin)</driver>
        <maxConnections>%%SharedResources/Connections/JDBC/BuildingDataConnection/maxConnections%%</maxConnections>
        <loginTimeout>%%SharedResources/Connections/JDBC/BuildingDataConnection/timeout%%</loginTimeout>
        <connectionType>JDBC</connectionType>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
        <location>%%SharedResources/Connections/JDBC/BuildingDataConnection/connectionString%%</location>
        <user>%%SharedResources/Connections/JDBC/BuildingDataConnection/userName%%</user>
        <password>%%SharedResources/Connections/JDBC/BuildingDataConnection/password%%</password>
    </config>
</BWSharedResource>