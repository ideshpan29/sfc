                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/BPM/SupportModule/GetCommentsData/CaseModelVersion"/>
                    </caseModelVersion>
                    <caseType>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/BPM/SupportModule/GetCommentsData/CaseType"/>
