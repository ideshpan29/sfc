<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/SFC_FOS_BPM/Processes/FOSBPMInterface/Adapter/FOSBPMInterface/SubProcesses/ReportGeneration/Schemas/Schema.xsd2" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:import namespace="http://www.tibco.com/schemas/SFC_FOS_BPM/Processes/FOSBPMInterface/Adapter/FOSBPMInterface/SubProcesses/ReportGeneration/Schemas/Schema.xsd2" schemaLocation="/Processes/FOSBPMInterface/Adapter/FOSBPMInterface/SubProcesses/ReportGeneration/Schemas/searchProcessInstanceSchema.xsd"/>
    <pd:name>Processes/FOSBPMInterface/Adapter/FOSBPMInterface/SubProcesses/ReportGeneration/Processes/searchProcessInstance.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:Request"/>
    <pd:startX>162</pd:startX>
    <pd:startY>96</pd:startY>
    <pd:returnBindings>
        <pfx:Response>
            <xsl:for-each select="$SQL-Direct/jdbcGeneralActivityOutput/unknownResultset/row">
                <pfx:Record>
                    <pfx:customer_name>
                        <xsl:value-of select="if(column[name=&quot;ATTRIBUTE3&quot;]/value!=&quot;&quot;) then column[name=&quot;ATTRIBUTE3&quot;]/value else &quot;NA&quot;"/>
                    </pfx:customer_name>
                    <pfx:process_instance>
                        <xsl:value-of select="if(column[name=&quot;PROCESS_INSTANCE&quot;]/value!=&quot;&quot;) then column[name=&quot;PROCESS_INSTANCE&quot;]/value else &quot;NA&quot;"/>
                    </pfx:process_instance>
                    <pfx:sibelcircuitId>
                        <xsl:value-of select="if(column[name=&quot;ATTRIBUTE2&quot;]/value!=&quot;&quot;) then column[name=&quot;ATTRIBUTE2&quot;]/value else &quot;NA&quot;"/>
                    </pfx:sibelcircuitId>
                    <pfx:project_manager>
                        <xsl:value-of select="if(column[name=&quot;ATTRIBUTE9&quot;]/value!=&quot;&quot;) then column[name=&quot;ATTRIBUTE9&quot;]/value else &quot;NA&quot;"/>
                    </pfx:project_manager>
                    <pfx:cur_activity>
                        <xsl:value-of select="if(column[name=&quot;CUR_ACTIVITY&quot;]/value!=&quot;&quot;) then column[name=&quot;CUR_ACTIVITY&quot;]/value else &quot;NA&quot;"/>
                    </pfx:cur_activity>
                    <pfx:status>
                        <xsl:value-of select="column[name=&quot;STATUS&quot;]/value"/>
                    </pfx:status>
                    <pfx:exec_mills>
                        <xsl:value-of select="if(column[name=&quot;EXEC_MILLIS&quot;]/value!='') then column[name=&quot;EXEC_MILLIS&quot;]/value else &quot;NOT COMPLETED&quot;"/>
                    </pfx:exec_mills>
                </pfx:Record>
            </xsl:for-each>
        </pfx:Response>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:Response"/>
    <pd:endX>454</pd:endX>
    <pd:endY>99</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1509953050277</pd:targetNamespace>
    <pd:activity name="SQL Direct">
        <pd:type>com.tibco.plugin.jdbc.JDBCGeneralActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCGeneralActivity</pd:resourceType>
        <pd:x>295</pd:x>
        <pd:y>99</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <jdbcSharedConfig>/SharedResources/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
        </config>
        <pd:inputBindings>
            <jdbcGeneralActivityInput>
                <statement>
                    <xsl:value-of select="concat(&quot;select PROCESS_INSTANCE, attribute3,attribute2,attribute9,cur_activity,status,EXEC_MILLIS from &quot;,$_globalVariables/ns:GlobalVariables/BPM/ProcessAnalysis/BPMDatabasename,&quot;.EC_PE_STATUS where &quot;, $Start/pfx:Request/pfx:key,&quot;='&quot;,$Start/pfx:Request/pfx:value,&quot;' &quot;)"/>
                </statement>
            </jdbcGeneralActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>SQL Direct</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SQL Direct</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>