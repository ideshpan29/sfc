    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>853</pd:x>
        <pd:y>107</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <cdataSections/>
            <term>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx2:RequestMetaDataElement" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </term>
        </config>
        <pd:inputBindings>
            <root>
                <xsl:for-each select="$Parse-XML/pfx2:RequestMetaDataElement">
                    <pfx2:RequestMetaDataElement>
                        <xsl:if test="bussinessTransactionID">
                            <bussinessTransactionID>
                                <xsl:value-of select="bussinessTransactionID"/>
                            </bussinessTransactionID>
                        </xsl:if>
                        <xsl:if test="correlationID">
                            <correlationID>
                                <xsl:value-of select="correlationID"/>
                            </correlationID>
                        </xsl:if>
                        <xsl:if test="orderID">
                            <orderID>
                                <xsl:value-of select="orderID"/>
                            </orderID>
                        </xsl:if>
                        <xsl:if test="orderRefNumber">
                            <orderRefNumber>
                                <xsl:value-of select="orderRefNumber"/>
                            </orderRefNumber>
                        </xsl:if>
                        <xsl:if test="planID">
                            <planID>
                                <xsl:value-of select="planID"/>
                            </planID>
                        </xsl:if>
                        <xsl:if test="siebelCircuitID">
                            <siebelCircuitID>
                                <xsl:value-of select="siebelCircuitID"/>
                            </siebelCircuitID>
                        </xsl:if>
                        <xsl:if test="manual">
                            <manual>
                                <xsl:value-of select="manual"/>
                            </manual>
                        </xsl:if>
                        <xsl:if test="processName">
                            <processName>
                                <xsl:value-of select="processName"/>
                            </processName>
                        </xsl:if>
                        <xsl:if test="PVMID">
                            <PVMID>
                                <xsl:value-of select="PVMID"/>
                            </PVMID>
                        </xsl:if>
                        <xsl:if test="caseID">
                            <caseID>
                                <xsl:value-of select="caseID"/>
                            </caseID>
                        </xsl:if>
                        <xsl:if test="planItemID">
                            <planItemID>
                                <xsl:value-of select="planItemID"/>
                            </planItemID>
                        </xsl:if>
                    </pfx2:RequestMetaDataElement>
                </xsl:for-each>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="FindCase">
        <pd:type>com.tibco.plugin.soap.SOAPSendReceiveActivity</pd:type>
        <pd:resourceType>ae.activities.SOAPSendReceiveUI</pd:resourceType>
