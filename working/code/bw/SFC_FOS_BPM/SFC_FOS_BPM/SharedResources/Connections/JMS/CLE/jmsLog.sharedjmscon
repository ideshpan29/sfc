<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>jmsLog</name>
    <resourceType>ae.shared.JMSConnectionKey</resourceType>
    <config>
        <NamingEnvironment>
            <UseJNDI>true</UseJNDI>
            <ProviderURL>tcp://localhost:7222</ProviderURL>
            <NamingURL>%%SharedResources/Connections/JMS/CLE/JndiContextUrl%%</NamingURL>
            <NamingInitialContextFactory>%%SharedResources/Connections/JMS/CLE/JndiContextFactory%%</NamingInitialContextFactory>
            <TopicFactoryName>%%SharedResources/Connections/JMS/CLE/TopicConnectionFactory%%</TopicFactoryName>
            <QueueFactoryName>%%SharedResources/Connections/JMS/CLE/QueueConnectionFactory%%</QueueFactoryName>
            <NamingPrincipal>%%SharedResources/Connections/JMS/CLE/JndiUserName%%</NamingPrincipal>
            <NamingCredential>%%SharedResources/Connections/JMS/CLE/JndiPassword%%</NamingCredential>
        </NamingEnvironment>
        <ConnectionAttributes>
            <username>%%SharedResources/Connections/JMS/CLE/JmsUserName%%</username>
            <password>%%SharedResources/Connections/JMS/CLE/JmsPassword%%</password>
            <clientID/>
            <autoGenClientID>true</autoGenClientID>
        </ConnectionAttributes>
        <UseXACF>false</UseXACF>
    </config>
</BWSharedResource>