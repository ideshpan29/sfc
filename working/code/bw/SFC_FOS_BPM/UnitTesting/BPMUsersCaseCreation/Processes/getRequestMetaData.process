            <endpointURL>http://%%BPM/BPMAPICall/HostName%%:%%BPM/BPMAPICall/port%%/amxbpm/BusinessDataServices</endpointURL>
            <authScheme>NONE</authScheme>
        </config>
        <pd:inputBindings>
            <inputMessage>
                <ns2:FindCaseByCriteriaRequest>
                    <caseModelVersion>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/BPM/SupportModule/getRequestMetaData/caseModelVersion"/>
                    </caseModelVersion>
                    <caseType>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/BPM/SupportModule/getRequestMetaData/caseType"/>
                    </caseType>
                    <criteria>
                        <queryString>
                            <xsl:value-of select="concat(&quot;siebelCircuitID= '&quot;,$Start/root/SiebelCircuitID,&quot;' and processName= '&quot;,$Start/root/ProcessName,&quot;'&quot;)"/>
                        </queryString>
                    </criteria>
                </ns2:FindCaseByCriteriaRequest>
            </inputMessage>
        </pd:inputBindings>
