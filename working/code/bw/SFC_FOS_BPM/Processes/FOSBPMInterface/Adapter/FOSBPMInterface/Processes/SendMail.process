<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/SFC_FOS_BPM/SharedResources/SchemaDefinitions/Schema.xsd" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://services.bds.tibco.com/BusinessDataServices_1.2.0" xmlns:ns="http://api.bds.tibco.com" xmlns:pfx3="http://bizData.fosImpl.sfc.com/dataModel" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType">
    <xsd:import namespace="http://www.tibco.com/schemas/SFC_FOS_BPM/SharedResources/SchemaDefinitions/Schema.xsd" schemaLocation="/Processes/FOSBPMInterface/Adapter/FOSBPMInterface/Shared Resources/SchemaDefinitions/sfcfosbpmschema.xsd"/>
    <xsd:import namespace="http://bizData.fosImpl.sfc.com/dataModel" schemaLocation="/Processes/FOSBPMInterface/Adapter/FOSBPMInterface/Shared Resources/SchemaDefinitions/BPMBIZData.xsd"/>
    <wsdl:import namespace="http://services.bds.tibco.com/BusinessDataServices_1.2.0" location="/Processes/FOSBPMInterface/Adapter/FOSBPMInterface/Shared Resources/WSDL/BussinessdataService.wsdl"/>
    <pd:name>Processes/FOSBPMInterface/Adapter/FOSBPMInterface/Processes/SendMail.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:SentMailRequest"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx:SendMailResponse>
            <pfx:Status>
                <xsl:value-of select="&quot;SUCCESS&quot;"/>
            </pfx:Status>
        </pfx:SendMailResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:SendMailResponse"/>
    <pd:endX>784</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1528193223693</pd:targetNamespace>
    <pd:activity name="FindCustomerInformation">
        <pd:type>com.tibco.plugin.soap.SOAPSendReceiveActivity</pd:type>
        <pd:resourceType>ae.activities.SOAPSendReceiveUI</pd:resourceType>
        <pd:x>394</pd:x>
        <pd:y>70</pd:y>
        <config>
            <timeout>0</timeout>
            <soapAttachmentStyle>SwA</soapAttachmentStyle>
            <timeoutType>Seconds</timeoutType>
            <service>pfx2:BusinessDataServices_1.2.0</service>
            <servicePort>BusinessDataServices.soap</servicePort>
            <operation>findCaseByCID</operation>
            <soapAction>findCaseByCID</soapAction>
            <endpointURL>http://%%BPM/BPMAPICall/HostName%%:%%BPM/BPMAPICall/port%%/amxbpm/BusinessDataServices</endpointURL>
            <authScheme>NONE</authScheme>
        </config>
        <pd:inputBindings>
            <inputMessage>
                <ns:FindCaseByCIDRequest>
                    <caseModelVersion>
                        <xsl:value-of select="'2'"/>
                    </caseModelVersion>
                    <caseType>
                        <xsl:value-of select="'com.sfc.fosImpl.bizData.dataModel.Customer_Information'"/>
                    </caseType>
                    <casePayload>
                        <xsl:value-of select="$Render-XML/xmlString"/>
                    </casePayload>
                </ns:FindCaseByCIDRequest>
            </inputMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render XML">
        <pd:type>com.tibco.plugin.xml.XMLRendererActivity</pd:type>
        <pd:resourceType>ae.activities.XMLRendererActivity</pd:resourceType>
        <pd:x>283</pd:x>
        <pd:y>68</pd:y>
        <config>
            <renderAsText>text</renderAsText>
            <validateInput>false</validateInput>
            <cdataSections>
                <row expandedName="{http://bizData.fosImpl.sfc.com/dataModel}Customer_InformationElement"/>
            </cdataSections>
            <term ref="pfx3:Customer_InformationElement"/>
        </config>
        <pd:inputBindings>
            <pfx3:Customer_InformationElement>
                <siebelCircuitID>
                    <xsl:value-of select="$Start/pfx:SentMailRequest/pfx:SiebelCircuitID"/>
                </siebelCircuitID>
            </pfx3:Customer_InformationElement>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Send Mail">
        <pd:type>com.tibco.plugin.mail.MailPubActivity</pd:type>
        <pd:resourceType>ae.activities.MailActivityResource</pd:resourceType>
        <pd:x>581</pd:x>
        <pd:y>71</pd:y>
        <config>
            <newMimeSupport>true</newMimeSupport>
            <inputOutputVersion>5.2.0</inputOutputVersion>
            <host>%%BPM/SendMailOperation/MailHost%%:%%BPM/SendMailOperation/MailPort%%</host>
            <authenticate>true</authenticate>
            <username>%%BPM/SendMailOperation/AuthenticateUser%%</username>
            <password>%%BPM/SendMailOperation/UserPassword%%</password>
            <InputHeaders>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </InputHeaders>
        </config>
        <pd:inputBindings>
            <ns2:mailActivityInput>
                <Headers>
                    <Content-Type>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/BPM/SendMailOperation/MailContentType"/>
                    </Content-Type>
                </Headers>
            </ns2:mailActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Render XML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FindCustomerInformation</pd:from>
        <pd:to>Send Mail</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render XML</pd:from>
        <pd:to>FindCustomerInformation</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Send Mail</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>