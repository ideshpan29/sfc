                    </xsl:when>
                </xsl:choose>
                <ns3:bodyElement>
                    <bodyText>
                        <xsl:value-of select="$MailContent/root/Contents"/>
                    </bodyText>
                </ns3:bodyElement>
                <Headers>
                    <Content-Type>
                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/BPM/SendMailOperation/MailContentType"/>
                    </Content-Type>
                </Headers>
            </ns2:mailActivityInput>
        </pd:inputBindings>
